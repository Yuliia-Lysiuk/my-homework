

<a href="{{largeImageURL}}" class="photo-card">
     <img class="img" src="{{webformatURL}}" alt="{{tags}}" width="310" height="207" loading="lazy" />
     <div class="info">
         <p class="info-item">
             <b>Likes</b>
             {{likes}}
             </p>
         <p class="info-item">
             <b>Views</b>
             {{views}}
             </p>
         <p class="info-item">
             <b>Comments</b>
             {{comments}}
             </p>
         <p class="info-item">
             <b>Downloads</b>
             {{downloads}}
             </p>
         </div>
     </a>
   